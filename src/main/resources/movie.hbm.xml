<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.github.bpazy.model.Movie" table="movie">
        <id name="id">
            <generator class="identity"/>
        </id>
        <property name="title"/>
        <property name="ratingNum" column="rating_num"/>
        <property name="ratingPeople" column="rating_people"/>
    </class>
</hibernate-mapping>